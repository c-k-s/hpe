pipeline {
  agent any
        stages {
    
          stage('Git Checkout') {
            steps {
              echo "Begin checkout of the git project"
              }
            }
    
          stage('Example') {
            steps {
              echo 'Hello World'
              }
            }
  
          stage('Engine stop') {
            steps {
              echo "Do you want to stop the engine ? "
              }
            }
  
         }
    }
}
